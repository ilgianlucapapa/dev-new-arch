{
  "expo": {
    "name": "dev-new-arch",
    "slug": "dev-new-arch",
    "version": "1.0.0",
    "scheme": "dev-new-arch",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c424f3a1-513e-40ff-82cc-e4191ae4e687"
    },
    "orientation": "portrait",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "it.engds.dev-new-arch",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["dev-new-arch", "yourcegidmobilepos", "moncler-rfid-scanner"],
        "NSFaceIDUsageDescription": "This app uses the biometrics authentication.",
        "NSCameraUsageDescription": "Needs Camera Access",
        "NSMicrophoneUsageDescription": "Needs Mic Access",
        "NSSpeechRecognitionUsageDescription": "Needs Speech Access"
      }
    },
    "android": {
      "package": "com.example.withnewarch"
    },
    "extra": {
      "env": "development",
      "buildNumber": "1.0.0",
      "eas": {
        "projectId": "c424f3a1-513e-40ff-82cc-e4191ae4e687"
      }
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets//splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#232526"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "ALL"
        }
      ],
      ["expo-font", {}],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "CUSTOM: Allow $(PRODUCT_NAME) to access the microphone",
          "speechRecognitionPermission": "CUSTOM: Allow $(PRODUCT_NAME) to securely recognize user speech"
        }
      ]
    ]
  }
}
